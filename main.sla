            <ITEXT CH="Chapter 2. How to contribute to OpenStack"/>
            <trail/>
            <PageItemAttributes>
                <ItemAttribute Name="chapterTag" Type="" Value="CH.2" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
