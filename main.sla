            <para PARENT="HTML_code"/>
            <ITEXT CH="$ openstack endpoint create --region RegionOne \"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="  object-store admin http://192.168.0.2:8080/v1"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="+--------------+----------------------------------+"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| Field        | Value                            |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="+--------------+----------------------------------+"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| enabled      | True                             |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| id           | ebb72cd6851d4defabc0b9d71cdca69b |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| interface    | admin                            |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| region       | RegionOne                        |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| region_id    | RegionOne                        |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| service_id   | 75ef509da2c340499d454ae96a2c5c34 |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| service_name | swift                            |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| service_type | object-store                     |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="| url          | http://192.168.0.2:8080/v1        |"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="+--------------+----------------------------------+"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="ここで注意しなければならない点は、Endpointのアドレスである。公式ドキュメントではcontrollerと記載がある。これはSwiftのproxy-serverがcontrollerで稼働していることが前提となっている。proxy-serverを別サーバとして構築、あるいは、前段にロードバランサを用いる場合、このEndpointに記載するアドレスはproxy-server、もしくは、ロードバランサのIPアドレスである。ポートは8080に設定されているが、これは80に変更しても問題ない。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="上記では、proxy-serverのIP addressを用いて設定している。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="proxy-serverの設定"/>
            <para PARENT="節"/>
            <ITEXT CH="パッケージのインストール"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONTSIZE="8" CH="# apt-get install swift swift-proxy python-swiftclient \"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="  python-keystoneclient python-keystonemiddleware \"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="  memcached"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="GitHubからサンプルconfを取得して編集を加える。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="# curl -o /etc/swift/proxy-server.conf https://git.openstack.org/cgit/openstack/swift/plain/etc/proxy-server.conf-sample?h=stable/liberty"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="認証項目に関してのみ設定値に注意が必要である。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="[DEFAULT]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="bind_port = 8080"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="user = swift"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="swift_dir = /etc/swift"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[pipeline:main]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="pipeline = catch_errors gatekeeper healthcheck proxy-logging cache container_sync bulk ratelimit authtoken keystoneauth container-quotas account-quotas slo dlo versioned_writes proxy-logging proxy-server"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[app:proxy-server]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="use = egg:swift#proxy"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="account_autocreate = true"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[filter:keystoneauth]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="use = egg:swift#keystoneauth"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="operator_roles = admin,user"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[filter:authtoken]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="paste.filter_factory = keystonemiddleware.auth_token:filter_factory"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="auth_uri = http://192.168.0.13:5000"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="auth_url = http://192.168.0.13:35357"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="auth_plugin = password"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="project_domain_id = default"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="user_domain_id = default"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="project_name = service"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="username = swift"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="password = SWIFT_PASS"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="delay_auth_decision = true"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[filter:cache]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="use = egg:swift#memcache"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="..."/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="memcache_servers = 127.0.0.1:11211"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="公式ドキュメントでは、auth_uriとauth_urlにcontrollerと記載があるが、この項目は認証に関する項目であるため、keystoneが稼働しているサーバのアドレスに変更する必要がある。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="account-server、container-server、object-serverの下準備"/>
            <para PARENT="節"/>
            <ITEXT CH="Swiftを構築するにあたって、account-server、container-server、object-serverではroot領域とは別デバイスのパーティションが必要となる。例えば、object-serverではobject-replicatorというプロセスがデータ領域としての追加デバイスがマウントされているかどうかのチェックをしている。root領域しかない場合、プロセスの起動に失敗するため注意が必要である。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="sdb以降のデバイスを確保できる場合はそれを使ってXFSでフォーマットする。無い場合は、ループバックデバイスを用いる方法がある。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="ループバックデバイスを使って普通のファイルをXFSファイルシステムとしてマウントする"/>
            <para PARENT="節"/>
            <ITEXT CH="ちなみに、以下の方法はdevstackのソースコードをベースにしている。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="XFSパッケージのインストール"/>
            <para PARENT="節"/>
            <ITEXT CH="xfsprogsというパッケージが必要になる。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$ sudo apt-get install -y xfsprogs"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="ループバックデバイスとなるファイルの作成"/>
            <para PARENT="節"/>
            <ITEXT CH="ファイルを配置するディレクトリの作成と、touchによるファイル作成を行う。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONTSIZE="8" CH="$ sudo mkdir -p /srv/node/images"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="$ sudo touch /srv/node/images/file.img"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="truncateコマンドによりSparseファイルを作成する。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$ sudo truncate -s 50G /srv/node/images/file.img"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="ファイルシステムのフォーマット"/>
            <para PARENT="節"/>
            <ITEXT CH="XFSでフォーマットする"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONTSIZE="8" CH="$ sudo /sbin/mkfs.xfs -f -i size=1024 /srv/node/images/file.img"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="meta-data=/srv/node/images/file.img isize=1024   agcount=4, agsize=3276800 blks"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="         =                       sectsz=512   attr=2, projid32bit=0"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="data     =                       bsize=4096   blocks=13107200, imaxpct=25"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="         =                       sunit=0      swidth=0 blks"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="naming   =version 2              bsize=4096   ascii-ci=0"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="log      =internal log           bsize=4096   blocks=6400, version=2"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="         =                       sectsz=512   sunit=0 blks, lazy-count=1"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="realtime =none                   extsz=4096   blocks=0, rtextents=0"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="fileコマンドで確認すると以下のとおりになる。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT FONTSIZE="8" CH="$ file /srv/node/images/file.img"/>
            <para PARENT="HTML_code"/>
            <ITEXT FONTSIZE="8" CH="/srv/node/images/file.img: SGI XFS filesystem data (blksz 4096, inosz 1024, v2 dirs)"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="マウント"/>
            <para PARENT="節"/>
            <ITEXT CH="マウント先のディレクトリを作成する。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$ sudo mkdir -p /srv/node/d1"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="マウントする。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$ sudo mount -t xfs -o loop,noatime,nodiratime,nobarrier,logbufs=8 /srv/node/images/file.img /srv/node/d1"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="dfコマンドで確認する。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$ df -h | grep loop0"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="/dev/loop0       50G   33M   50G   1% /srv/node/d1"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="これで、ループバックデバイスのマウントが完了する。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="/etc/rsyncd.confを編集して以下を追記する。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="uid = swift"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="gid = swift"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="log file = /var/log/rsyncd.log"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="pid file = /var/run/rsyncd.pid"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="address = MANAGEMENT_INTERFACE_IP_ADDRESS"/>
            <para PARENT="HTML_code"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[account]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="max connections = 2"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="path = /srv/node/"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="read only = false"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="lock file = /var/lock/account.lock"/>
            <para PARENT="HTML_code"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[container]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="max connections = 2"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="path = /srv/node/"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="read only = false"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="lock file = /var/lock/container.lock"/>
            <para PARENT="HTML_code"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="[object]"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="max connections = 2"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="path = /srv/node/"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="read only = false"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="lock file = /var/lock/object.lock"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="MANAGEMENT_INTERFACE_IP_ADDRESSには、各ホストのIPアドレスを入れる。また、max connectionsの値は、増やすことをおすすめする。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="/etc/default/rsyncを開いてRSYNC_ENABLEをtrueにする。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="RSYNC_ENABLE=true"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="rsyncデーモンを起動する。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="# service rsync start"/>
            <para PARENT="HTML_code"/>
            <ITEXT CH="account-server、container-serverの設定"/>
            <para PARENT="節"/>
            <ITEXT CH="account-server、container-serverの稼働するホストで各々必要なパッケージをインストールする。注意する点は、memcachedをインストールして起動させておく必要がある点である。"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="# apt-get install swift swift-account swift-container memcached"/>
            <para PARENT="HTML_code"/>
